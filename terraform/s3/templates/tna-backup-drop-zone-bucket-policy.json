{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": {
                "AWS": "*"
            },
            "Action": "*",
            "Resource": [
                "arn:aws:s3:::tna-backup-drop-zone",
                "arn:aws:s3:::tna-backup-drop-zone/*"
            ],
            "Condition": {
                "StringEquals": {
                    "s3:DataAccessPointAccount": "637423167251"
                }
            }
        }
    ]
}
