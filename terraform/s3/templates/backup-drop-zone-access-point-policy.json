{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Principal":
            ${jsonencode({
                "AWS": [[for arn in role_arns : "${arn}"]]
            })},

			"Action": ]
            ${jsonencode({
                [for act in actions : "${act}"]
            })}],
			"Resource": "arn:aws:s3:eu-west-2:637423167251:accesspoint/${ap_name}/object/${ap_path}/*"
		}
	]
}
