{
    "Statement": [
        {
            "Action": [
                "s3:ListBucket"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::tna-backup-drop-zone",
                "arn:aws:s3:::tna-backup-vault",
                "arn:aws:s3:::tna-external-services-backup"
            ]
        },
        {
            "Action": [
                "s3:GetObject",
                "s3:GetObjectAttributes",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:BypassGovernanceRetention"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::tna-backup-drop-zone/*"
            ]
        },
        {
            "Action": [
                "s3:GetObject",
                "s3:GetObjectAttributes",
                "s3:PutObject",
                "s3:PutObjectAcl"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::tna-backup-vault/*",
                "arn:aws:s3:::tna-external-services-backup/*"
            ]
        },
        {
            "Action": [
                "ssm:GetParameter"
            ],
            "Effect": "Allow",
            "Resource": "arn:aws:ssm:eu-west-2:637423167251:parameter/application/backup/secure-backups"
        },
        {
            "Action": [
                "secretsmanager:GetSecretValue"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:secretsmanager:eu-west-2:637423167251:secret:application/lambda/backup-check-in-dCsJNp"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "sqs:SendMessage",
                "sqs:ReceiveMessage",
                "sqs:DeleteMessage",
                "sqs:GetQueueAttributes",
                "sqs:GetQueueUrl",
                "sqs:SetQueueAttributes"
            ],
            "Resource": [
                ${queue_arns}
            ]
        }
    ],
    "Version": "2012-10-17"
}
