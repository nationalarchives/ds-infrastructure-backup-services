{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::tna-backup-drop-zone",
                "arn:aws:s3:::tna-backup-vault"
            ],
            "Sid": "DropZoneListBucket"
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:GetObjectAttributes",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:BypassGovernanceRetention"
            ],
            "Resource": [
                "arn:aws:s3:::tna-backup-drop-zone/*"
            ],
            "Sid": "DropZoneS3Access"
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:GetObjectAttributes",
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:PutObjectLegalHold"
            ],
            "Resource": [
                "arn:aws:s3:::tna-backup-vault/*"
            ],
            "Sid": "DropZoneS3AccessDefaultBackupVault"
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:PutObjectLegalHold"
            ],
            "Resource": [
                "arn:aws:s3:::bv-digital-services/*",
                "arn:aws:s3:::bv-ds-digital-files/*"
            ],
            "Sid": "DropZoneS3WriteAccessBackupVaults"
        }
    ]
}
